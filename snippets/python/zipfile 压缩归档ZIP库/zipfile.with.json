{
  "zipfile.extractall 解压全部文件到": {
    "prefix": ["zipfile extractall", "extractall -zipfile"],
    "body": ["extractall(${1:out_path})  # 解压全部文件到"],
    "description": ["zipfile.extractall 解压全部文件到"]
  },
  "with ZipFile -r 解压文件到": {
    "prefix": ["with zipfile ZipFile -r"],
    "body": [
      "with zipfile.ZipFile(zip_path, \"r\") as zipf:",
      "    zipf.extractall(${1:out_path})  # 解压到目录"
    ],
    "description": ["with ZipFile -r 解压文件到"]
  },
  "with ZipFile -w 压缩文件到": {
    "prefix": ["with zipfile ZipFile -w"],
    "body": [
      "with zipfile.ZipFile(out_zip, \"w\", zipfile.ZIP_DEFLATED) as zipf:",
      "    zipf.writestr(${1:file_path})  # 压缩文件到目录"
    ],
    "description": ["with ZipFile -w 压缩文件到"]
  }
}
