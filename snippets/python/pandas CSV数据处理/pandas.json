    "description": [
      "pandas.values 删除指定列",
      "** labels: 列索引列表",
      "** axis: 0 行索引, 1 列索引",
      "** inplace: 是否修改原对象"
    ]
  },
  "pandas.values 值": {
    "prefix": ["pd values", "pandas values", "values -pd", "values -pandas"],
