    "description": "创建 function 方法函数"
  },
  "弹窗": {
    "prefix": ["prt -alert", "alert"],
    "body": ["alert(${1:content})", ""],
    "description": ["弹窗"]
  },
  "导出语法": {
    "prefix": ["export default", "expt default -export"],
    "body": ["export default { $0 }", ""],
    "description": ["导出语法", "将一个对象、函数、类或值作为模块的默认导出(暴露)，供其他文件引入使用"]
  },
  "var 变量-函数作用域": {
    "prefix": ["var", "new var"],
    "body": ["var $0"],
    "description": ["var 变量-函数作用域"]
  },
  "let 变量-块级作用域": {
    "prefix": ["let", "new let"],
    "body": ["let $0"],
    "description": ["let 变量-块级作用域"]
  },
  "const 常量-块级作用域": {
    "prefix": ["const", "new const"],
