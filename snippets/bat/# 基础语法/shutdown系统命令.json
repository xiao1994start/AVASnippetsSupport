{
  "关机": {
    "prefix": ["shutdown -Shutdown"],
    "body": ["shutdown -s -t ${1:10} -f"],
    "description": "关机"
  },
  "重启": {
    "prefix": ["shutdown -Restart"],
    "body": ["shutdown -r -t ${1:10}"],
    "description": "重启"
  },
  "注销": {
    "prefix": ["shutdown -Logout"],
    "body": ["shutdown -l -t ${1:10}"],
    "description": "注销"
  },
  "定时关机": {
    "prefix": ["shutdown -At"],
    "body": ["at ${1:23:00} shutdown -s"],
    "description": "定时关机"
  },
  "取消所有的shutdown命令": {
    "prefix": ["shutdown -All"],
    "body": ["shutdown -a"],
    "description": "取消所有的shutdown命令"
  }
}
