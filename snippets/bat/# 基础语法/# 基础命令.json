{
  "BAT.tmpl": {
    "prefix": ["xxx"],
    "body": [
      "ECHO OFF",
      "setlocal EnableDelayedExpansion",
      "CD /D \"%~dp0\"",
      "",
      "",
      "",
      "$0",
      "",
      "",
      "",
      "endlocal",
      "goto :eof",
      "ECHO. & ECHO 退出",
      "@REM TIMEOUT /t 1 > NUL & EXIT /B",
      ""
    ],
    "description": ["BAT.tmpl"]
  },
  "ECHO 控制台输出": {
    "prefix": ["prt -ECHO", "ECHO"],
    "body": ["ECHO $0"],
    "description": ["ECHO 控制台输出"]
  },
  "ECHO 控制台空行": {
    "prefix": ["prt.-ECHO", "nn"],
    "body": ["ECHO.", "$0"],
    "description": ["ECHO 控制台空行"]
  },
  "暂停": {
    "prefix": ["pause", "pass"],
    "body": ["PAUSE"],
    "description": ["暂停"]
  },
  "暂停(无提示)": {
    "prefix": ["pause NUL", "pass NUL"],
    "body": ["PAUSE > NUL"],
    "description": ["暂停(无提示)"]
  },
  "运行程序": {
    "prefix": ["start"],
    "body": ["start ${2| ,\"Windows窗口\"|} ${1:\"dst运行程序_或_网址\"}"],
    "description": ["运行程序(第一个双引号为空->等待运行程序关闭继续)"]
  },
  "等待n秒后继续": {
    "prefix": ["timeout"],
    "body": ["timeout /T ${1:1} > NUL"],
    "description": ["等待n秒后继续"]
  },
  "CMD /C": {
    "prefix": ["cmd -C"],
    "body": ["CMD /C ${1:命令}"],
    "description": ["CMD /C"]
  },
  "start CMD /K": {
    "prefix": ["cmd -K"],
    "body": ["start CMD /K ${1:命令}"],
    "description": ["在新的终端窗口执行"]
  },
  "ECHO | clip 设置剪切板内容": {
    "prefix": ["clip -ECHO"],
    "body": ["ECHO 设置剪切板内容| clip"],
    "description": ["ECHO | clip 设置剪切板内容"]
  },
  "使用记事本打开host文件": {
    "prefix": ["Host Edit"],
    "body": [
      "@REM *取消 hosts 只读",
      "attrib -R C:\\WINDOWS\\system32\\drivers\\etc\\hosts",
      "@REM *用 记事本 打开 hosts  ",
      "call notepad C:\\WINDOWS\\system32\\drivers\\etc\\hosts",
      "@REM *设定 hosts 只读",
      "attrib +R C:\\WINDOWS\\system32\\drivers\\etc\\hosts"
    ],
    "description": ["使用记事本打开host文件"]
  },
  "使用cmd静默安装innosetup安装包": {
    "prefix": ["inno -Install"],
    "body": ["${1:InnoSetup安装程序路径} /SILENT /COMPONENTS=\"${2:InnoExtractor提取的安装参数_使用英文逗号隔开}\""],
    "description": ["使用cmd静默安装innosetup安装包"]
  },
  "msi安装_静默": {
    "prefix": ["msi -Install"],
    "body": ["@REM msiexec /i [安装] \"安装包路径\" /qb [指定在安装过程中有一个基本 UI]", "msiexec /i ${1:安装包路径} /qb"],
    "description": ["msi安装_静默"]
  },
  "msi卸载_静默": {
    "prefix": ["msi -X", "msi -Uninstall"],
    "body": ["@REM msiexec /x [卸载] \"安装包路径\" /qb [指定在安装过程中有一个基本 UI]", "msiexec /x ${1:安装包路径} /qb"],
    "description": ["msi卸载_静默"]
  },
  "分割线": {
    "prefix": ["line"],
    "body": [
      "ECHO.",
      "ECHO ---------------------------------------------------------------------------------------------------------------------------------------------",
      "ECHO.",
      ""
    ],
    "description": ["分割线"]
  }
}
